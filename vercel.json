{
    "version": 2,
    "builds": [
      { "src": "api/quote.py", "use": "@vercel/python" },
      { "src": "api/quote_all.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/api/quote", "dest": "api/quote.py" },
      { "src": "/api/quote/all", "dest": "api/quote_all.py" }
    ]
  }